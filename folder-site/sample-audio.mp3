</span><span><span class="hljs-tag">&lt;<span class="hljs-name">img</span></span></span><span> </span><span><span class="hljs-attr">id</span></span><span>=</span><span><span class="hljs-string">"imagine"</span></span><span> </span><span><span class="hljs-attr">src</span></span><span>=</span><span><span class="hljs-string">"img.png"</span></span><span> </span><span><span class="hljs-attr">width</span></span><span>=</span><span><span class="hljs-string">"100"</span></span><span> </span><span><span class="hljs-attr">draggable</span></span><span>=</span><span><span class="hljs-string">"true"</span></span><span> </span><span><span class="hljs-attr">ondragstart</span></span><span>=</span><span><span class="hljs-string">"start(event)"</span></span><span>&gt;
</span><span><span class="hljs-tag">&lt;<span class="hljs-name">div</span></span></span><span> </span><span><span class="hljs-attr">id</span></span><span>=</span><span><span class="hljs-string">"zona"</span></span><span> </span><span><span class="hljs-attr">ondrop</span></span><span>=</span><span><span class="hljs-string">"drop(event)"</span></span><span> </span><span><span class="hljs-attr">ondragover</span></span><span>=</span><span><span class="hljs-string">"allowDrop(event)"</span></span><span> </span><span><span class="hljs-attr">style</span></span><span>=</span><span><span class="hljs-string">"width:150px;height:150px;border:1px solid black;"</span></span><span>&gt;</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">div</span></span></span><span>&gt;

</span><span><span class="hljs-tag">&lt;<span class="hljs-name">script</span></span></span><span>&gt;<span class="language-javascript">
</span></span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">allowDrop</span></span><span>(</span><span><span class="hljs-params">ev</span></span><span>) {
  ev.</span><span><span class="hljs-title function_">preventDefault</span></span><span>();
}
</span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">start</span></span><span>(</span><span><span class="hljs-params">ev</span></span><span>) {
  ev.</span><span><span class="hljs-property">dataTransfer</span></span><span>.</span><span><span class="hljs-title function_">setData</span></span><span>(</span><span><span class="hljs-string">"text"</span></span><span>, ev.</span><span><span class="hljs-property">target</span></span><span>.</span><span><span class="hljs-property">id</span></span><span>);
