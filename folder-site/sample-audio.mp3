</span></span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">getLocation</span></span><span>(</span><span><span class="hljs-params"></span></span><span>) {
  </span><span><span class="hljs-keyword">if</span></span><span> (navigator.</span><span><span class="hljs-property">geolocation</span></span><span>) {
    navigator.</span><span><span class="hljs-property">geolocation</span></span><span>.</span><span><span class="hljs-title function_">getCurrentPosition</span></span><span>(afiseazaLocatia);
  } </span><span><span class="hljs-keyword">else</span></span><span> {
    </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"locatie"</span></span><span>).</span><span><span class="hljs-property">innerText</span></span><span> = </span><span><span class="hljs-string">"Geolocația nu este suportată."</span></span><span>;
  }
}
</span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">afiseazaLocatia</span></span><span>(</span><span><span class="hljs-params">position</span></span><span>) {
  </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"locatie"</span></span><span>).</span><span><span class="hljs-property">innerText</span></span><span> =
    </span><span><span class="hljs-string">"Latitudine: "</span></span><span> + position.</span><span><span class="hljs-property">coords</span></span><span>.</span><span><span class="hljs-property">latitude</span></span><span> +
