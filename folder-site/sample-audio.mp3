      </span><span><span class="hljs-keyword">const</span></span><span> nume = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"numeInput"</span></span><span>).</span><span><span class="hljs-property">value</span></span><span>;
